<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fruithapnotifier.app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TestActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EventNotificationDetailActivity"
            android:label="@string/title_eventnotification_detail"
            android:parentActivityName=".EventNotificationListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EventNotificationListActivity" />
        </activity>

        <service
            android:name=".service.NotificationService"
            android:enabled="true"
            android:exported="false" >
        </service>
    </application>

</manifest>
